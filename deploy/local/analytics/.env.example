# Umami Analytics Environment Variables
# Copy this file to .env and update with your values

# PostgreSQL Configuration
POSTGRES_PASSWORD=change_this_strong_password_32_chars

# Umami Configuration
# Generate a random 32-character string: openssl rand -hex 16
APP_SECRET=change_this_random_32_char_string

# Default Umami login: admin/umami (change immediately after first login)
